<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.akingyin.greendaotest">
  <permission android:name="com.zlcdgroup.provider.READ"
      android:label="读取权限"
  />
  <permission android:name="com.zlcdgroup.provider.WIRTE"
      android:label="写取权限"
  />
  <permission android:name="com.zlcdgroup.provider_auth"
      android:label="共享权限"/>
  <uses-permission android:name="com.zlcdgroup.provider.READ"/>
  <uses-permission android:name="com.zlcdgroup.provider.WIRTE"/>
  <uses-permission android:name="com.zlcdgroup.provider_auth"/>
  <application
      android:allowBackup="true"
      android:icon="@mipmap/ic_launcher"
      android:label="@string/app_name"
      android:supportsRtl="true"
      android:theme="@style/AppTheme">
    <activity android:name=".MainActivity">
      <intent-filter>
        <action android:name="android.intent.action.MAIN"/>

        <category android:name="android.intent.category.LAUNCHER"/>
      </intent-filter>
    </activity>
  </application>

</manifest>